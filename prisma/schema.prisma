generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Admin {
  id         Int     @id @default(autoincrement())
  uuid       String  @unique @default(uuid())
  name       String
  email      String  @unique
  password   String
  isViewOnly Boolean
}

model Signup {
  id   Int    @id @default(autoincrement())
  uuid String @unique @default(uuid())
}
